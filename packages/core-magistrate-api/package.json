{
    "name": "@swipechain/core-magistrate-api",
    "version": "3.0.5",
    "description": "Magistrate API for Swipechain Core",
    "license": "MIT",
    "contributors": [
        "Swipechain <community@swipe.org>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "types": "dist/index",
    "scripts": {
        "build": "yarn clean && yarn compile",
