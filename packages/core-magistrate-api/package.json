        "prepublishOnly": "yarn build",
        "pretest": "bash ../../scripts/pre-test.sh"
    },
    "dependencies": {
        "@hapi/boom": "^9.0.0",
        "@hapi/hapi": "^20.0.3",
        "@swipechain/core-api": "^3.0.5",
        "@swipechain/core-kernel": "^3.0.5",
        "@swipechain/core-magistrate-crypto": "^3.0.5",
        "@swipechain/core-magistrate-transactions": "^3.0.5",
        "@swipechain/crypto": "^3.0.5",
        "joi": "^17.3.0"
    },
    "devDependencies": {
        "@types/hapi__boom": "^7.4.1",
        "@types/hapi__joi": "^17.0.0"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    },
    "swipechain": {
        "core": {
            "alias": "magistrate-api"
        }
    },
