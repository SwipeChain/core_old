    "dependencies": {
        "@swipechain/core-database": "^3.0.5",
        "@swipechain/core-kernel": "^3.0.5",
        "@swipechain/core-transactions": "^3.0.5",
        "@swipechain/crypto": "^3.0.5",
        "immutable": "^4.0.0-rc.12",
        "joi": "^17.3.0"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    },
    "swipechain": {
        "core": {
            "alias": "state"
        }
    },
