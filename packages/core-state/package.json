{
    "name": "@swipechain/core-state",
    "version": "3.0.5",
    "description": "State Management for Swipechain Core",
    "license": "MIT",
    "contributors": [
        "Swipechain <community@swipe.org>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "types": "dist/index",
    "scripts": {
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
